<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:PrismTestV2.Themes"
    xmlns:customerItemVm="using:PrismTestV2.ViewModels.Customers"
    xmlns:customerItemV="using:PrismTestV2.Views.Customers"
    >

    <DataTemplate x:Key="CustomerItemPage">
        <Grid Background="Red" Margin="0,0,0,15">
            <StackPanel Orientation="Vertical">
                <TextBlock Text="{Binding Name}" Foreground="Black"/>
                <TextBlock Text="{Binding Surname}" Foreground="Black"/>
                <TextBlock Text="{Binding Surname}" Foreground="Black"/>
            </StackPanel>
        </Grid>
        <!--<customerItemV:CustomerItemPage />-->
    </DataTemplate>

    <!--<DataTemplate customerItemVm:CustomerItemViewModel>
        <customerItemV:CustomerItemPage />
    </DataTemplate>-->

</ResourceDictionary>

